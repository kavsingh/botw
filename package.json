{
  "name": "botw",
  "version": "1.0.0",
  "description": "Keeping track of them shrines.",
  "author": "Kav Singh <flakisms@gmail.com> (https://github.com/kavsingh)",
  "license": "ISC",
  "repository": {
    "url": "git@github.com:kavsingh/basics-react-express.git",
    "type": "git"
  },
  "build": {
    "appId": "com.kav.botw",
    "files": [
      "distclient/**/*",
      "server/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "publish": null
  },
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node server/index.js",
    "dev": "NODE_ENV=development nodemon server/index.js",
    "build": "rimraf clientbundle && NODE_ENV=production webpack --colors --progress",
    "lint:js": "eslint .",
    "lint:css": "stylelint .",
    "test": "BABEL_DISABLE_CACHE=1 NODE_ENV=test jest --config jest.config.json",
    "test:coverage": "npm t -- --coverage",
    "test:watch": "npm t -- --watch",
    "test:viewreport": "open coverage/lcov-report/index.html",
    "test:full": "npm run test:coverage -- --verbose && npm run test:viewreport",
    "prod": "npm run build && npm run start",
    "dist": "ELECTRON=true electron-builder",
    "edev": "ELECTRON=true electron .",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "common-tags": "^1.4.0",
    "css-loader": "^0.28.4",
    "electron": "^1.6.11",
    "electron-builder": "^19.13.0",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-mongrel-react": "kavsingh/eslint-config-mongrel-react.git#3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-lodash-fp": "^2.1.3",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "history": "^4.6.3",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.29.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "lodash": "^4.17.4",
    "postcss-cssnext": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "next",
    "react-router-redux": "next",
    "react-test-renderer": "^15.6.1",
    "recursive-readdir": "^2.2.1",
    "redux": "^3.7.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "sqlite": "^2.8.0",
    "style-loader": "^0.18.2",
    "styled-jsx": "^1.0.8",
    "stylelint": "^7.12.0",
    "stylelint-config-standard": "^16.0.0",
    "svg-inline-loader": "^0.7.1",
    "webpack": "^3.1.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "dependencies": {
    "express-ipfilter": "^0.2.5",
    "nodemon": "^1.11.0"
  }
}
